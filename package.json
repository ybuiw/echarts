{
  "name": "@template/typescript",
  "version": "6.8.1",
  "description": "Use an example of TypeScript.",
  "private": true,
  "scripts": {
    "start": "cross-env PORT=6002 kkt start",
    "build": "kkt build",
    "test": "kkt test --env=jsdom",
    "test:coverage": "kkt test --env=jsdom --coverage",
    "type": "npm run t:cjs && npm run t:esm",
    "t:esm": "lerna exec --scope @ahwecharts/* -- tsbb types --outDir lib/esm --target ESNEXT",
    "t:cjs": "lerna exec --scope @ahwecharts/* -- tsbb types --outDir lib/cjs --target ESNEXT",
    "b:css": "lerna exec --scope @ahwecharts/* -- compile-less -d src -o lib/esm",
    "b:lib": "lerna exec --scope @ahwecharts/* -- tsbb build --target react",
    "watch": "lerna exec --scope @ahwecharts/* -- tsbb watch --target react",
    "b": "npm run b:lib && npm run type && npm run b:css",
    "clean": "lerna clean",
    "publish": "lerna publish from-package"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kktjs/kkt.git"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "echarts": "^5.0.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "web-vitals": "1.1.0"
  },
  "devDependencies": {
    "@kkt/less-modules": "6.8.1",
    "@types/jest": "26.0.20",
    "@types/react": "17.0.3",
    "@types/react-dom": "17.0.3",
    "kkt": "6.8.1",
    "webpack": "^4.44.2",
    "webpack-cli": "^4.5.0",
    "compile-less-cli": "^1.6.0",
    "tsbb": "^2.0.5",
    "lerna": "^3.22.1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
